<h1>Authors</h1>
<%= link_to 'New Author', '/authors/new' %>
<% @authors.each_with_index do |author, index| %>
  <div id="index-<%= index %>">
    <p><%= author.name %>, Created At: <%= author.created_at %> 
<%= link_to 'Delete Author', "/authors/#{author.id}", method: :delete %> 
  <%= link_to 'Update Author', "/authors/#{author.id}/edit" %></p>
    </div>
<% end %>
